.searchresult .dbtype {
  width: 16.66%;
  height: 48rpx;
  line-height: 48rpx;
}

.searchresult .dbtype.active {
  color: #ff510d;
  background: #f7f7f7;
  border-radius: 24rpx;
}

.searchresult .list_zb {
  position: fixed;
  bottom: 0;
  top: 201rpx;
}

.liveresult .channel_img {
  width: 132rpx;
  height: 132rpx;
}

.liveresult .channel_right {
  height: 152rpx;
  line-height: 66rpx;
  margin-left: 152rpx;
  padding-right: 120rpx;
}

.liveresult .channel_play {
  width: 120rpx;
  height: 152rpx;
  top: 0;
}

.liveresult .channel_play image {
  right: 40rpx;
  top: 66rpx;
  width: 40rpx;
  height: 41rpx;
}

.push-button {
  background-color: #ff510d;
  color: #fff;
  width: 132rpx;
  height: 57rpx;
  line-height: 57rpx;
  border-radius: 6rpx;
}

.backbtn {
  background: #1ed699;
  width: 52rpx;
  height: 28rpx;
  line-height: 28rpx;
  font-size: 18rpx;
  border-top-left-radius: 5px;
  border-bottom-right-radius: 5px;
}
