/*直播台标  */
.tv_tb_part{position:absolute;bottom:0;width:100%;height:50rpx;font-size:26rpx;line-height:50rpx;}
/* .tv_tb_part text{position:absolute;bottom:auto;overflow:hidden;margin-left:10rpx;width:170rpx!important;text-overflow:ellipsis;white-space:nowrap;} */
.tv_tb_part text{position:absolute;bottom:auto;overflow:hidden;right:0;text-overflow:ellipsis;white-space:nowrap;color: #fff;text-align:right;padding: 0 8rpx;background-image:linear-gradient(0deg, rgba(34, 34, 34, 0.6), rgba(34, 34, 34, 0));}
.tv_tb_part image{margin-left:20rpx;width:50rpx!important;height:50rpx!important}
.tv_tb_part .pre{position:absolute;bottom:0;width:100%;background:#ffefe5}

/*加载  */
._loading{width:100%;height:100rpx;border-top:1px solid #f0f0f0;color:#a4a4a4;text-align:center;font-size:26rpx;line-height:100rpx}


/* 按钮 */
button{border-radius:0;background:0 0}
button::after{border:none;background:0 0}

/* 无网络时的模块样式 */
.no-network{width:420rpx;height:230rpx;margin:424rpx auto;}
.no-network .bg-img{width: 100rpx;height: 70rpx;margin: 0 auto}
.no-network .t{margin: 48rpx 0 28rpx 0;line-height: 28rpx}
.no-network .btn{height:56rpx;width:160rpx;border-radius:16rpx;background:linear-gradient(to left, rgb(255,81,13) , rgb(255,112,0));padding-left:10px;line-height:55rpx;}
/*注册，登录*/
.resgister{padding: 30rpx 56rpx 0;}
.resgister .login{padding:20rpx 0 20rpx 20rpx; }
.resgister .reg_title{font-size: 48rpx;padding:48rpx 0 55rpx;}
.resgister .reg_title image{width:38rpx;margin-right: 14rpx;}
.resgister .placehold{font-size:36rpx;color:#ccc;}
.resgister .inputbox .item{height:148rpx;padding-top: 28rpx;line-height:120rpx;box-sizing: border-box;}
.resgister .deleteimg{width: 30rpx;padding:30rpx 12rpx 30rpx 20rpx;}
.resgister .inputbox .pwdimg{width: 34rpx;padding:30rpx 12rpx 30rpx 20rpx;}
.resgister .regbtn{background: linear-gradient(to right, #ff510d, #ff7000);font-size: 36rpx;margin-top: 70rpx;}
.resgister .deletehisimg{width: 24rpx;padding:30rpx 12rpx 30rpx 20rpx;}
.resgister .phonehis{top:170rpx;left:0;z-index:10;box-shadow:0 0 6px rgba(68,68,68,0.1);padding:0 19rpx;}
.resgister .phonehis .itemhis{height:84rpx;line-height: 84rpx;border-bottom:1px solid #eee;}
.resgister .phonehis .itemhis:last-child{border-bottom:none;}
/*弹出框*/
.zan-dialog__header{font-weight: 700}
.zan-dialog__content{min-height: auto;}
.zan-dialog__content--title{color: #222;text-align: center} 
/*搜索框*/
.searchRank{position:relative;width:100%;height:60rpx;}
.searchRank .form{position:absolute;top:0;bottom:0;margin:auto;width:100%;height:60rpx;}
.searchRank .form .cl{margin:0 auto;padding-left:64rpx;width:710rpx;height:60rpx;border:none;border-radius:30rpx;background:#f3f3f3;color:#aaaaaa;font-size:24rpx;line-height:60rpx;box-sizing: border-box;}
.searchRank icon{position:absolute;top:14rpx;left:40rpx}

.actor-text{width: 320rpx}

/* 订购弹窗 */
.buy-list{width:100%;height:100%;position:fixed;z-index:999;}
.buy-list .mask{ width:100%;height:100%;position:fixed;opacity:.5;background:#ccc;}
.buy-list .content{position:relative;z-index:1000;height:630rpx;width:78%;background:#fff;margin:35% 0 0 11%;border-radius:30rpx;}
.close-box{position:absolute; top:20rpx; right:20rpx; width:45rpx; height:45rpx;}
.buy-list .img{ position:absolute; top:16rpx; right:18rpx; width:35rpx; height:35rpx;}
.buy-list .t{position:absolute;right:36%;top:45rpx;font-weight: bold;text-align:center;width: 60%;left: 18%}
.buy-list .cont{position:absolute;width:100%; margin:20% 0 0 0;height:410rpx;border-bottom: 1rpx solid #edece8;border-top: 1rpx solid #edece8}
.buy-list .cont .item{width:98%;border-bottom:1px solid #edece8;height:120rpx;margin-bottom:20rpx;}
.buy-list .cont .item .t1{color: #ddaf60;margin:20rpx 0 10rpx 0;}
.buy-list .cont .active{color:#ff5555 !important;font-weight: bold}
.buy-list .content .btn-box{position:absolute;height: 65rpx;bottom: 25rpx;display: flex;justify-content:space-between;width: 60%;margin-left: 18%}
.buy-list .content .btn-box .btn{width:100%;height: 65rpx;line-height: 65rpx;border-radius: 12rpx;background: #ff3131;color: white;text-align: center;}